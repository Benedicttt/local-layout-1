    "jquery": "^3.3.1",
    "js-yaml": "^3.12.0",
    "merge-yaml": "^1.0.0",
    "protractor-html-reporter-2": "^1.0.4",
    "protractor-http-client": "^1.0.4",
    "require-all": "^3.0.0",
    "webdriver": "^5.0.0-alpha.7",
    "webdriverio": "^4.13.2",
